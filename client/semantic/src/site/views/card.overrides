/*******************************
    User Variable Overrides
*******************************/
.ui.cards > .card > .content,
.ui.card > .content {
    overflow-x: hidden;
}

.ui.cards > .card > .content > .description,
.ui.card > .content > .description {
    height: 5em;
    margin-bottom: 1em;
}

.ui.cards a.card,
.ui.link.cards .card,
a.ui.card,
.ui.link.card {
    color: inherit;
}